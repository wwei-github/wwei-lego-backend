{
  "name": "wwei-lego-backend",
  "version": "1.0.0",
  "description": "hackernews showcase using typescript && egg",
  "private": true,
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "egg-scripts start",
    "dev": "egg-bin dev",
    "ets": "npx ets -w",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "pnpm run lint -- --fix && pnpm run test-local",
    "cov": "egg-bin cov",
    "tsc": "tsc -p tsconfig.json",
    "ci": "pnpm run lint && pnpm run cov && pnpm run tsc",
    "lint": "eslint .",
    "clean": "tsc -b --clean"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "egg": "^3.11.0",
    "egg-bcrypt": "^1.1.0",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.3.1",
    "egg-redis": "^2.5.0",
    "egg-scripts": "^2.17.0",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.3.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.22.0",
    "mongoose": "^7.2.2",
    "mongoose-sequence": "^5.3.1"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.1.0",
    "@types/cheerio": "^0.22.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "cheerio": "^1.0.0-rc.2",
    "egg-bin": "^5.9.0",
    "egg-mock": "^5.5.0",
    "egg-ts-helper": "^1.34.7",
    "eslint": "^8.31.0",
    "eslint-config-egg": "^12.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
